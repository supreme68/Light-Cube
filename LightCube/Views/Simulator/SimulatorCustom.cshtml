

@{
    ViewBag.Title = "SimulatorCustom";
}



 @foreach (var item in Model )
 {
<ul>
    <li>
        @item.Ohms
    </li>
</ul>
 }
<td>
    @*@Html.DisplayFor(modelItem => item.Information)*@
</td>



<link rel="stylesheet" href="~/Content/css/Simulator.css" />

    <nav class="navbar" id="navigation">
        <button id="btn1" draggable="true" class="btn btn-primary" onclick="Resistor()">Resistor </button><br />
        <button draggable="true" class="btn btn-primary" onclick="Led()">LED </button><br />
        <button draggable="true" class="btn btn-primary" onclick="Battery()">Battery </button><br />
        <button draggable="true" class="btn btn-primary" onclick="Wire()">Wire</button><br />
        <button id="btn1" draggable="true" class="btn btn-success">On</button><br />
        <button draggable="true" class="btn btn-danger">Off</button><br />
    </nav>

    <div id="stats">
        <div class="test2">
            <div class="row"></div>
            <p id="test" class="col-sm-12">Stats</p>
            <p class="col-sm-12">Resistance:<span id="resistance"></span></p>
            <p class="col-sm-12">Voltage:<span id="voltage"></span></p>
            <p class="col-sm-12">Current:<span id="current"></span></p>
            <p class="col-sm-12">Description:<span id="description"></span></p>
            @Html.ActionLink("Ready Circuits", "SimulatorReady", "Simulator", null, new { @class = "btn btn-primary" })
        </div>
    </div>
    <script src="~/Scripts/phaser.js"></script>
    <script src="~/Content/js/SimulatorCustom.js"></script>

